#tab {
    overflow: hidden;
    zoom: 1;
    width: 100%;
    text-align: center;
    background-color: #FBFBFF;

    top: 0px;
    left: 0px;
    padding: 0px;
    margin: 0px;

}

#tab li {
    float: left;
    /* cursor:pointer;*/
    list-style-type: none;
    text-align: center;
    line-height: 60px;
    height: 60px;
    width: 33%;
    font-size: 16px;
}

#tab li div {

    height: 100%;
    text-align: center;
    margin-top: 10px;
    display: inline-block;
}

#warnText {
font-size: 16px;
font-weight: 100;
margin-top: 0px;
width: 100%;
color: #666;
text-indent: 0.5em;
}

